<?xml version="1.0"?>
<package>

  <metadata>
    <id>Scanbot.Xamarin.SDK.Dependencies</id>
    <version>3.3.2</version>
    <title>Scanbot SDK Dependencies</title>
    <description>A collection of dependencies required by Scanbot SDK and Scanbot Barcode Scanner SDK (not meant to be used as a separate NuGet package)</description>
    <authors>Scanbot</authors>
    <owners>Scanbot</owners>
    <projectUrl>https://github.com/doo/xamarin-third-party-android-bindings</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <copyright>Copyright 2020 doo GmbH</copyright>
    <dependencies>
      <group targetFramework="MonoAndroid81">

        <dependency id="Xamarin.AndroidX.Core"                       version="1.3.2.3"/>
        <dependency id="Xamarin.AndroidX.AppCompat"                  version="1.2.0.7"/>
        <dependency id="Xamarin.AndroidX.ConstraintLayout"           version="2.0.4.2"/>
        <dependency id="Xamarin.AndroidX.Arch.Core.Runtime"          version="2.1.0.8"/>
        <dependency id="Xamarin.AndroidX.DynamicAnimation"           version="1.0.0.7"/>
        <dependency id="Xamarin.AndroidX.DataBinding.ViewBinding"    version="4.2.1"  />

        <dependency id="Xamarin.Google.Android.Material"             version="1.3.0.1"/>
        <dependency id="Xamarin.AndroidX.Legacy.Support.V4"          version="1.0.0.7"/>
        
        <dependency id="Xamarin.Kotlin.StdLib"                       version="1.4.20" />
        <dependency id="Xamarin.Kotlin.StdLib.Common"                version="1.4.20" />
        <dependency id="Xamarin.Kotlin.StdLib.Jdk7"                  version="1.4.20" />
        <dependency id="Xamarin.Google.Dagger"                       version="2.37.0" />
        <dependency id="Square.OkIO"                                 version="2.8.0"  />

        <dependency id="Xamarin.Android.ReactiveX.RxAndroid"         version="2.1.1.1" />
        <dependency id="Xamarin.Android.ReactiveX.RxJava"            version="2.2.10.1"/>

      </group>
    </dependencies>
  </metadata>

  <files>
    <file src="bin/Release/Scanbot.Xamarin.SDK.Dependencies.dll" target="lib/MonoAndroid81" />
    <file src="../Scanbot.Xamarin.ApwLibrary/bin/Release/Scanbot.Xamarin.ApwLibrary.dll" target="lib/MonoAndroid81" />
    <file src="THIRD-PARTY-NOTICES.txt" target="THIRD-PARTY-NOTICES.txt" />
  </files>

</package>
