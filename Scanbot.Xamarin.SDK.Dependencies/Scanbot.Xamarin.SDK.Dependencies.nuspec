        <dependency id="Xamarin.AndroidX.Activity.Ktx" version="1.2.2" />
        <dependency id="Xamarin.AndroidX.Collection.Ktx" version="1.1.0" />
        <dependency id="Xamarin.AndroidX.Fragment.Ktx" version="1.3.3" /> 
        <dependency id="Xamarin.AndroidX.Core" version="1.3.2.3" />
